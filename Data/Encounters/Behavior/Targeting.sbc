        [MatchAllFilters:Relation]
        [MatchAllFilters:Powered]

        [MatchNoneFilters:Underwater]
        [MinUnderWaterDepth:150]

        [Owners:Player]
        [Owners:NPC]
        [Relations:Enemy]

        [GetTargetBy:ClosestDistance]

      </Description>

    </EntityComponent>
    
  </EntityComponents>
